{
  "name": "dooray-ai",
  "version": "0.1.0",
  "description": "AI-powered CLI tool for automated development workflow with Dooray! integration",
  "main": "dist/index.js",
  "bin": {
    "dooray-ai": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "cli",
    "automation",
    "ai",
    "dooray",
    "git",
    "development",
    "workflow"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.6.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "ora": "^5.4.1",
    "simple-git": "^3.20.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
